{
  "type": "module",
  "dependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.7.9",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.0.1",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "formidable": "^2.0.1",
    "jasmine": "^4.3.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "pug": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node ./dist/server.js",
    "testDB": "db-migrate --env placeholder db:create estore",
    "migrateUp": "cross-env ENV=dev db-migrate --env dev up",
    "migrateDown": "cross-env ENV=dev db-migrate --env dev down",
    "test": "tsc && cross-env ENV=test jasmine",
    "migrateUpTest": "cross-env ENV=test db-migrate --env test up",
    "migrateDownTest": "cross-env ENV=test db-migrate --env test down",
    "migrateTest": "npm run build && npm run migrateUpTest && (npm run test || npm run migrateDownTest) && npm run migrateDownTest"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/formidable": "^2.0.5",
    "@types/jasmine": "^4.3.0",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12"
  }
}
